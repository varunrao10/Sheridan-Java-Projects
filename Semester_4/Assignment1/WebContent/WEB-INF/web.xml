<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Assignment1</display-name>
  <servlet>
    <servlet-name>AcademicStandingServlet</servlet-name>
    <servlet-class>sheridan.college.assignments.AcademicStandingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GpaCalculator</servlet-name>
    <servlet-class>sheridan.college.assignments.GpaCalculator</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AcademicStandingServlet</servlet-name>
    <url-pattern>/AcademicStandingServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GpaCalculator</servlet-name>
    <url-pattern>/GpaCalculator</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>